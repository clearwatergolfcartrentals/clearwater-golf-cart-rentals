{
  "name": "clearwater-golf-cart-rentals",
  "version": "1.0.0",
  "description": "Clearwater Golf Cart Rentals Website",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc && npm run build:css",
    "build:css": "tailwindcss -i ./src/styles/input.css -o ./dist/public/styles.css",
    "build:pages": "npm run build:css && mkdir -p docs && cp -r src/public/* docs/ && cp dist/public/styles.css docs/ && cp CNAME docs/",
    "start": "node dist/server.js",
    "watch:css": "tailwindcss -i ./src/styles/input.css -o ./dist/public/styles.css --watch"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "nodemon": "^3.0.2",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "private": true
}
